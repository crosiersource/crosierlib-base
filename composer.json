{
    "name": "crosiersource/crosierlib-base",
    "description": "Pacote base do Crosier",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Eduardo Pauluk",
            "email": "carlospauluk@gmail.com"
        }
    ],
    "require": {
        "roave/security-advisories": "dev-master",
        "php": "^7.1.3",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-intl": "*",
        "php-http/guzzle6-adapter": "^1.1",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^4.3",
        "symfony/console": "^4.3",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/web-server-bundle": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/security-bundle": "^4.3",
        "symfony/orm-pack": "*",
        "symfony/debug-bundle": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^4.3",
        "symfony/form": "^4.3",
        "easycorp/easy-log-handler": "^1.0.2",
        "sensiolabs/security-checker": "^5.0",
        "symfony/http-foundation": "^4.3",
        "symfony/cache": "^4.3",
        "symfony/serializer-pack": "*"
    },
    "autoload": {
        "psr-4": {
            "CrosierSource\\CrosierLibBaseBundle\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "security-checker security:check": "script",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.*"
        }
    }
}
