{
    "name": "crosiersource/crosierlib-base",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Eduardo Pauluk",
            "email": "carlospauluk@gmail.com"
        }
    ],
    "require": {
        "php": "7.4.*",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-yaml": "*",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "6.5.8",
        "php-http/guzzle6-adapter": "^2.0",
        "symfony/cache": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/debug-bundle": "5.3.*",
        "symfony/doctrine-messenger": "5.3.*",
        "symfony/form": "5.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "5.3.*",
        "symfony/security-guard": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/validator": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "scienta/doctrine-json-functions": "~4.1",
        "api-platform/core": "2.6.8",
        "symfony/messenger": "^5.3",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/mailer": "^5.3",
        "doctrine/orm": "^2.10",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "symfony/orm-pack": "^2.2",
        "symfony/semaphore": "5.3.*"
    },
    "autoload": {
        "psr-4": {
            "CrosierSource\\CrosierLibBaseBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
