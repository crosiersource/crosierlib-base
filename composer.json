{
    "name": "crosiersource/crosierlib-base",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Eduardo Pauluk",
            "email": "carlospauluk@gmail.com"
        }
    ],
    "require": {
        "php":"7.4.*",
        "ext-bcmath":"*",
        "ext-ctype":"*",
        "ext-curl":"*",
        "ext-dom":"*",
        "ext-gd":"*",
        "ext-iconv":"*",
        "ext-intl":"*",
        "ext-json":"*",
        "ext-simplexml":"*",
        "ext-soap":"*",
        "ext-yaml":"*",
        "ext-zip":"*",
        "ext-zlib":"*",
        "api-platform/core":"2.7.10",
        "doctrine/annotations":"^1.0",
        "doctrine/doctrine-bundle":"^2.4",
        "doctrine/doctrine-migrations-bundle":"^3.2",
        "doctrine/orm":"^2.10",
        "friendsofsymfony/jsrouting-bundle":"^2.2",
        "guzzlehttp/guzzle":"6.5.8",
        "nelmio/cors-bundle":"^2.1",
        "phpdocumentor/reflection-docblock":"^5.3",
        "php-http/guzzle6-adapter":"^2.0",
        "scienta/doctrine-json-functions":"~4.1",
        "sensio/framework-extra-bundle":"^5.4",
        "symfony/asset":"5.4.*",
        "symfony/cache":"5.4.*",
        "symfony/console":"5.4.*",
        "symfony/debug-bundle":"5.4.*",
        "symfony/doctrine-messenger":"5.4.*",
        "symfony/dotenv":"5.4.*",
        "symfony/expression-language":"5.4.*",
        "symfony/flex":"^1.3.1",
        "symfony/form":"5.4.*",
        "symfony/framework-bundle":"5.4.*",
        "symfony/http-client":"5.4.*",
        "symfony/intl":"5.4.*",
        "symfony/mailer":"^5.4",
        "symfony/messenger":"^5.4",
        "symfony/mime":"5.4.*",
        "symfony/monolog-bundle":"^3.1",
        "symfony/notifier":"5.4.*",
        "symfony/process":"5.4.*",
        "symfony/property-access":"5.4.*",
        "symfony/property-info":"5.4.*",
        "symfony/proxy-manager-bridge":"5.4.*",
        "symfony/rate-limiter":"5.4.*",
        "symfony/runtime":"5.4.*",
        "symfony/security-bundle":"5.4.*",
        "symfony/security-guard":"5.4.*",
        "symfony/semaphore":"5.4.*",
        "symfony/serializer":"5.4.*",
        "symfony/string":"5.4.*",
        "symfony/translation":"5.4.*",
        "symfony/twig-bundle":"5.4.*",
        "symfony/validator":"5.4.*",
        "symfony/web-link":"5.4.*",
        "symfony/webpack-encore-bundle":"^1.12",
        "symfony/yaml":"5.4.*",
        "twig/extra-bundle":"^2.12|^3.0",
        "twig/intl-extra":"^3.2",
        "twig/twig":"^2.12|^3.0",
        "dompdf/dompdf": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "CrosierSource\\CrosierLibBaseBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*",
            "docker": false
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
