<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="crosier_source.crosier_lib_base_bundle.twig.route_exists_extension" class="CrosierSource\CrosierLibBaseBundle\Twig\RouteExistsExtension">
            <argument type="service" id="service_container" />
            <tag name="twig.extension" />
        </service>
        <service id="CrosierSource\CrosierLibBaseBundle\Twig\RouteExistsExtension" alias="crosier_source.crosier_lib_base_bundle.twig.route_exists_extension"></service>


        <service id="crosier_source.crosier_lib_base_bundle.business.config.stored_view_info_business" class="CrosierSource\CrosierLibBaseBundle\Business\Config\StoredViewInfoBusiness">

        </service>
        <service id="CrosierSource\CrosierLibBaseBundle\Business\Config\StoredViewInfoBusiness" alias="crosier_source.crosier_lib_base_bundle.business.config.stored_view_info_business" />

        <service id="crosier_source.crosier_lib_base_bundle.business.security.security_business" class="CrosierSource\CrosierLibBaseBundle\Business\Security\SecurityBusiness">
            <argument type="service" id="doctrine" />
            <argument type="service" id="security.authorization_checker" />
        </service>
        <service id="CrosierSource\CrosierLibBaseBundle\Business\Security\SecurityBusiness" alias="crosier_source.crosier_lib_base_bundle.business.security.security_business" />


        <service id="crosier_source.crosier_lib_base_bundle.repository.config.app_repository" class="CrosierSource\CrosierLibBaseBundle\Repository\Config\AppRepository" />
        <service id="CrosierSource\CrosierLibBaseBundle\Repository\Config\AppRepository" alias="crosier_source.crosier_lib_base_bundle.repository.config.app_repository" />

    </services>
</container>