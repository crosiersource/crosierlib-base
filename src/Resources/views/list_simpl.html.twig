{% extends '@CrosierLibBase/index.html.twig' %}



{% block page_content %}


    <div class="card" style="margin-bottom: 50px;">
        <div class="card-header">

            <div class="d-flex flex-wrap align-items-center">

                <div class="mr-1">
                    {% if page_title is defined %}
                        <h3>{{ page_title }}</h3>
                    {% endif %}
                    {% if page_subTitle is defined %}
                        <h6>{{ page_subTitle }}</h6>
                    {% endif %}
                </div>

                <div class="d-sm-flex flex-nowrap ml-auto">

                    <a role="button" class="btn btn-primary ml-1" title="Novo"
                       href="{{ url('cln_procedimento_form') }}">
                        <i class="fas fa-file" aria-hidden="true"></i>
                    </a>

                </div>
            </div>
        </div>

        <div class="card-body">

            <div class="row">
                <div class="col">
                    {% include '@CrosierLibBase/Util/flashesOnAlerts.html.twig' %}
                </div>
            </div>

            <div class="table-responsive-sm">

                <form name="form_{{ listId }}" id="form_{{ listId }}">
                    <table class="table table-sm table-striped table-hover crsr-datatable display compact" id="{{ listId }}">
                        {% block corpoTabela %}
                        {% endblock %}
                    </table>
                </form>

            </div>

            {% block aposTabela %}
            {% endblock %}

        </div>
    </div>


{% endblock %}



{% block down_scripts %}
    {% if listJS is defined %}
        <script src="{{ asset('build/' ~ listJS) }}"></script>
    {% endif %}
{% endblock %}